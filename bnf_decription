<c-structure> ::= <sentence>
<sentence> ::= <constituent> | <sentence> <constituent>
<constituent> ::= <lexical-item> | <phrase>
<phrase> ::= <constituent> <constituent>
<f-structure> ::= <f-structure-avm>
<f-structure-avm> ::= <attribute> = <value> | <f-structure-avm> <attribute> = <value>
<attribute> ::= SUBJ | OBJ | COMPL | <other-attribute>
<value> ::= <lexical-item> | <phrase> | <disjunctive-f-structure>
<disjunctive-f-structure> ::= <f-structure> | <f-structure> | <f-structure>
<correspondence> ::= <c-structure-element> : <f-structure-element>
<c-structure-element> ::= <constituent>
<f-structure-element> ::= <attribute-value-matrix>
<functional-uncertainty> ::= <underspecification>
<underspecification> ::= <partial-f-structure>
<partial-f-structure> ::= <f-structure-avm> | <disjunctive-f-structure>
<lexical-item> ::= <lexical-entry>
<lexical-entry> ::= <syntactic-properties> <semantic-properties> <phonological-properties>
<syntactic-properties> ::= <syntactic-feature> | <syntactic-properties> <syntactic-feature>
<semantic-properties> ::= <semantic-feature> | <semantic-properties> <semantic-feature>
<phonological-properties> ::= <phonological-feature> | <phonological-properties> <phonological-feature>
<syntactic-feature> ::= <constituent-type> | <grammatical-function>
<constituent-type> ::= NOUN | VERB | ADJ | <other-constituent-type>
<grammatical-function> ::= SUBJ | OBJ | COMPL
<semantic-feature> ::= <semantic-role> | <semantic-relation>
<semantic-role> ::= AGENT | THEME | <other-semantic-role>
<semantic-relation> ::= CAUSE | MODIFY | <other-semantic-relation>
<phonological-feature> ::= <phoneme> | <phonological-rule>
<phoneme> ::= <vowel> | <consonant>
<phonological-rule> ::= <sound-change-rule>
<sound-change-rule> ::= <context> -> <sound>
<context> ::= <phoneme> | <phoneme> <phoneme>
<sound> ::= <phoneme>
